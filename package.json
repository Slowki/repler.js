{
  "name": "repler",
  "version": "0.0.1-alpha.5",
  "description": "An auto-reloading REPL with Babel support",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "babel src --out-dir dist",
    "build:watch": "npm run build -- -w",
    "prepublishOnly": "npm run clean && npm run build",
    "publish": "echo TODO"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slowki/repler.js.git"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "repler": "dist/cli.js"
  },
  "keywords": [
    "repl"
  ],
  "author": "Stephan Wolski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Slowki/repler.js/issues"
  },
  "homepage": "https://github.com/Slowki/repler.js#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-types": "^6.26.0",
    "chalk": "^2.3.2",
    "chokidar": "^2.0.2",
    "fs-extra": "^5.0.0",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.66.0"
  }
}
